{
	"schemaVersion": 1,
	"id": "quark",
	"version": "${version}",
	"name": "Quark Fabric",
	"description": "Small things, improving Minecraft bit by bit.",
	"authors": [
		"Vazkii, WireSegal, MCVinnyq, Sully"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
